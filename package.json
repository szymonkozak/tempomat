{
  "name": "tempomat",
  "version": "2.0.0-beta.0",
  "description": "Tempo.io cloud CLI",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc -p .",
    "test": "TZ=Europe/Warsaw jest",
    "lint": "eslint --fix --ext .ts ."
  },
  "author": "Szymon Kozak @szymonkozak",
  "license": "MIT",
  "homepage": "https://github.com/szymonkozak/tempomat",
  "bugs": "https://github.com/szymonkozak/tempomat/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/szymonkozak/tempomat.git"
  },
  "bin": {
    "tempo": "./bin/run"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "keywords": [
    "tempo",
    "tempo-cli",
    "cli",
    "tempo cloud",
    "tempo.io",
    "jira"
  ],
  "oclif": {
    "topics": {
      "alias": {
        "description": "manage issue key aliases"
      },
      "tracker": {
        "description": "manage issue time trackers (beta)"
      }
    },
    "commands": "./dist/commands",
    "bin": "tempo",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-autocomplete"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 7
    }
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/command": "^1.8.36",
    "@oclif/config": "^1.18.17",
    "@oclif/plugin-autocomplete": "^0.1.5",
    "@oclif/plugin-help": "^2.2.3",
    "@oclif/plugin-warn-if-update-available": "^1.7.3",
    "@types/lodash": "^4.17.16",
    "@types/node": "^13.13.52",
    "axios": "^1.8.4",
    "chalk": "^3.0.0",
    "cli-table3": "^0.5.1",
    "cli-truncate": "^2.1.0",
    "cli-ux": "^5.4.5",
    "date-fns": "^2.30.0",
    "globby": "^10.0.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@oclif/dev-cli": "^1.26.10",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
